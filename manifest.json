{
    "manifest_version": 2,
    "name": "e_Forte",
    "version": "1.0",
    "description": "url analysis",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "https://www.whoisxmlapi.com/*",
        "chrome://favicon/",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ],

    "content_security_policy": "script-src 'self'; object-src 'self'; img-src chrome://favicon;",

    "browser_action": {
        "default_icon": "16.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery-3.1.0.min.js", "popup.js"]
    }]
}
